# Use the following starter code to begin creating a conceptual ERD --
departments as dep
-
dept_no varchar(4) PK
dept_name varchar UNIQUE


dept_emp 
-
emp_no int PK FK >- emp.emp_no
dept_no varchar(4) PK FK >- dep.dept_no


dept_manager  
-
emp_no int FK >- emp.emp_no
dept_no varchar(4) PK FK >- dep.dept_no


employees as emp
-
emp_no int PK
emp_title_id varchar(15)
birth_date varchar(10)
first_name varchar
last_name varchar
sex varchar(1)
hire_date varchar(10)


salaries 
-
emp_no int PK FK >- emp.emp_no
salary int


titles 
-
emp_title_id int PK FK >- emp.emp_title_id
title varchar
