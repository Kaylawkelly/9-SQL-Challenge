# Use the following starter code to begin creating a conceptual ERD --

departments as dep
-
dept_no varchar(4) PK
dept_name varchar UNIQUE


dept_emp as de
-
emp_no int PK FK >- emp.emp_no
dept_no varchar(4) PK FK >- dep.dept_no
from_date varchar(10) PK
to_date varchar(10)


dept_manager as dm
-
dept_no varchar(4) PK FK >- dep.dept_no
emp_no int FK >- emp.emp_no
from_date varchar(10) PK
to_date varchar(10)


employees as emp
-
emp_no int PK
birth_date varchar(10)
first_name varchar
last_name varchar
gender varchar(1)
hire_date varchar(10)


titles 
-
emp_no int PK FK >- emp.emp_no
title varchar
from_date varchar(10) PK
to_date varchar(10)


salaries 
-
emp_no int PK FK >- emp.emp_no
salary int
from_date varchar(10)
to_date varchar(10)